  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "mongodb": "^3.2.3",
    "mongoose": "^5.4.22",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.11",
    "react": "^16.8.6",
